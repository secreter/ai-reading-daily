{
  "name": "ai-reading-hub",
  "description": "All in AI，看见未来。 每天精选最值得读的AI文章，帮你筛掉时代的噪音。 以简讯见广度，以深读见洞察。 技术、趋势、思考，一站式掌握AI世界。",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:clean-dev": "vuepress dev docs --clean-cache",
    "docs:dev": "vuepress dev docs",
    "docs:update-package": "npx vp-update",
    "gen": "node ./scripts/fetch_ai_articles.js",
    "gen:prod": "NODE_ENV=production node ./scripts/fetch_ai_articles.js && npm run docs:build"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.20",
    "@vuepress/plugin-blog": "2.0.0-rc.86",
    "@vuepress/theme-default": "2.0.0-rc.88",
    "sass-embedded": "^1.86.0",
    "vue": "^3.5.13",
    "vuepress": "2.0.0-rc.20"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
}
